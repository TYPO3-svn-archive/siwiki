<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<mainSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.mainSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<storageFolder>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.storageFolderSelection</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</storageFolder>				
				</el>
			</ROOT>
		</mainSheet>
		<rteSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.rteSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<rteWidth>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.rteWidth</label>
							<config>
								<type>input</type>
                                                                <size>10</size>
                                                                <eval>num</eval>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </rteWidth>
					<rteHeight>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.rteHeight</label>
							<config>
								<type>input</type>
                                                                <size>10</size>
                                                                <eval>num</eval>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </rteHeight>
				</el>
			</ROOT>
		</rteSheet>
		<miscSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.miscSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<enableGraphviz>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.enableGraphviz</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enableGraphviz>
					<anonymous>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.anonymous</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</anonymous>
					<fullTextSearch>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.fullTextSearch</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
                                        </fullTextSearch>
					<adminMail>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.adminMail</label>
							<config>
								<type>input</type>
                                                                <size>30</size>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </adminMail>
				</el>
			</ROOT>
		</miscSheet>
	</sheets>
</T3DataStructure>

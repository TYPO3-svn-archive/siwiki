<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<mainSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.mainSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<controller>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.controllerSwitch</label>
							<config>
								<type>select</type>
									<items type="array">
 										<numIndex index="5" type="array">
											<numIndex index="0">LLL:EXT:siwiki/locallang_db.xml:flexform.controllerSwitch.wiki</numIndex>
											<numIndex index="1">wiki</numIndex>
										</numIndex>
 										<numIndex index="10" type="array">
											<numIndex index="0">LLL:EXT:siwiki/locallang_db.xml:flexform.controllerSwitch.latestUpdates</numIndex>
											<numIndex index="1">latestUpdates</numIndex>
										</numIndex>
								</items>
 							</config>
						</TCEforms>
					</controller>

					<storageFolder>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.storageFolderSelection</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</storageFolder>				
				</el>
			</ROOT>
		</mainSheet>
		<rteSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.rteSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<rteWidth>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.rteWidth</label>
							<config>
								<type>input</type>
                                                                <size>10</size>
                                                                <eval>num</eval>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </rteWidth>
					<rteHeight>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.rteHeight</label>
							<config>
								<type>input</type>
                                                                <size>10</size>
                                                                <eval>num</eval>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </rteHeight>
				</el>
			</ROOT>
		</rteSheet>
		<miscSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.miscSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<enableFilemanager>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.enableFilemanager</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enableFilemanager>
					<enableGraphviz>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.enableGraphviz</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enableGraphviz>
					<anonymous>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.anonymous</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</anonymous>
					<fullTextSearch>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.fullTextSearch</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
                                        </fullTextSearch>
					<adminMail>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.adminMail</label>
							<config>
								<type>input</type>
                                                                <size>30</size>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </adminMail>
				</el>
			</ROOT>
		</miscSheet>
		<updatesSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:siwiki/locallang_db.xml:flexform.updatesSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
                                        <wikiPage>
                                                <TCEforms>
                                                <exclude>1</exclude>
                                                <label>LLL:EXT:siwiki/locallang_db.xml:flexform.wikiPage</label>
                                                <config>
                                                        <type>group</type>
                                                        <internal_type>db</internal_type>
                                                        <allowed>pages</allowed>
                                                        <size>1</size>
                                                        <maxitems>1</maxitems>
                                                        <minitems>0</minitems>
                                                        <show_thumbs>1</show_thumbs>
                                                </config>
                                                </TCEforms>
                                        </wikiPage>

					<updatesLimit>
						<TCEforms>
							<label>LLL:EXT:siwiki/locallang_db.xml:flexform.updatesLimit</label>
							<config>
								<type>input</type>
                                                                <size>5</size>
                                                                <eval>num</eval>
                                                                <checkbox></checkbox>
							</config>
						</TCEforms>
                                        </updatesLimit>
				</el>
			</ROOT>
		</updatesSheet>
	</sheets>
</T3DataStructure>
